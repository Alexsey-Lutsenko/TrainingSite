{
  "name": "trainingsite.git",
  "version": "1.0.0",
  "description": "fullstack app with angular and express",
  "main": "index.js",
  "scripts": {
    "start": "node index",
    "server": "nodemon index",
    "client-install": "npm install --prefix client",
    "client": "npm run start --prefix client",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "heroku-postbuild": "NPM_CONFIG_PRODUCTION=false npm run client-install && npm run build --prefix client"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Alexsey-Lutsenko/TrainingSite.git"
  },
  "keywords": [
    "express",
    "angular"
  ],
  "author": "Alexsey Lutsenko",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Alexsey-Lutsenko/TrainingSite/issues"
  },
  "homepage": "https://github.com/Alexsey-Lutsenko/TrainingSite#readme",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "jsonwebtoken": "^8.5.1",
    "moment": "^2.29.1",
    "mongoose": "^5.11.3",
    "morgan": "^1.10.0",
    "multer": "^1.4.2",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0"
  },
  "devDependencies": {
    "concurrently": "^5.3.0",
    "nodemon": "^2.0.6"
  }
}
